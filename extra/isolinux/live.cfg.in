LABEL ${DISTRO} Live (${ABINAME}-${FLAVOR})
  SAY "Booting ${DISTRO} Live (English)"
  linux /live/vmlinuz
  initrd /live/initrd.img
  APPEND boot=live ${COMPONENTS}

MENU begin other languages
	menu title ^Other languages...
	label otherlangs
		menu label ^Back..
		menu exit
	include otherlangs.cfg
menu end
